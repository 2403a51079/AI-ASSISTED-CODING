      "source": [
        "class Warehouse:\n",
        "    def __init__(self):\n",
        "        self.products = []\n",
        "\n",
        "    def add_product(self, product):\n",
        "        if isinstance(product, Product):\n",
        "            self.products.append(product)\n",
        "        else:\n",
        "            print(\"Invalid product type.\")\n",
        "\n",
        "    def list_products(self):\n",
        "        for product in self.products:\n",
        "            print(f\"Product: {product.name}, Price: {product.price}, Quantity: {product.quantity}\")\n",
        "\n",
        "    def display_most_valuable_product(self):\n",
        "        if not self.products:\n",
        "            print(\"The warehouse is empty.\")\n",
        "            return\n",
        "\n",
        "        most_valuable_product = max(self.products, key=lambda product: product.calculate_value())\n",
        "        print(f\"The most valuable product is: {most_valuable_product.name} with a total value of {most_valuable_product.calculate_value()}\")"
      ],
      "execution_count": 18,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "f5a852d3"
      },
      "source": [
        "warehouse = Warehouse()"
      ],
      "execution_count": 19,
      "outputs": []
    },
