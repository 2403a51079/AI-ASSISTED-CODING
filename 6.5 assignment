        "class Product:\n",
        "    def __init__(self, name, price, quantity):\n",
        "        self.name = name\n",
        "        self.price = price\n",
        "        self.quantity = quantity\n",
        "\n",
        "    def calculate_value(self):\n",
        "        return self.price * self.quantity"
      ],
      "execution_count": 15,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "c1fb8a4a"
      },
      "source": [
        "laptop = Product(\"Laptop\", 1200, 5)"
      ],
      "execution_count": 16,
      "outputs": []
    },
